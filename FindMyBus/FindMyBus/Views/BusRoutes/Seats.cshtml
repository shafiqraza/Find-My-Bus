@model FindMyBus.Models.BusBooking

@{
    ViewBag.Title = "Seats";
}
<div class="container">

    @ViewBag.error
    <div class="alert alert-dismissible alert-success mt-2">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        <strong>Well done!</strong> You Reservation successfully Compeleted!
    </div>


    <hr />

    <table class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Seats</th>
                <th>Status</th>
                <th>Date Time</th>
                <th>User Name</th>
                <th>Bus Name</th>
                <th>Route</th>

            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@Model.NoOfSeats</td>
                <td>@Model.Status</td>
                <td>@Model.DateTime</td>
                <td>@Model.LoginId</td>
                <td>@Model.BusId</td>
                <td>@Model.BusRoutesId</td>
            </tr>
        </tbody>
    </table>
    @*Seats: @Model.NoOfSeats<br />
        Status: @Model.Status <br />
        Date: @Model.DateTime <br />
        Login: @Model.LoginId <br />
        Bus: @Model.BusId <br />
        Route: @Model.BusRoutesId <br />*@




</div>